<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="portal_my_home_employee_project" name="Show Employee Project" inherit_id="portal.portal_my_home">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Employee Projects</t>
                <t t-set="url" t-value="'/my/employee_projects'"/>
                <t t-set="placeholder_count" t-value="'employee_projects_count'"/>
            </t>
        </xpath>
    </template>
</odoo>
